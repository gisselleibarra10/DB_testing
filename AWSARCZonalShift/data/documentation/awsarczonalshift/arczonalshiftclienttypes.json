Shift/ARCZonalShiftClientTypes/ValidationExceptionReason":{"role":"symbol","title":"ARCZonalShiftClientTypes.ValidationExceptionReason","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"ValidationExceptionReason"}],"abstract":[],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/ValidationExceptionReason","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ValidationExceptionReason"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/validationexceptionreason"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes/ConflictExceptionReason":{"role":"symbol","title":"ARCZonalShiftClientTypes.ConflictExceptionReason","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"ConflictExceptionReason"}],"abstract":[],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/ConflictExceptionReason","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ConflictExceptionReason"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/conflictexceptionreason"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes/AppliedStatus":{"role":"symbol","title":"ARCZonalShiftClientTypes.AppliedStatus","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"AppliedStatus"}],"abstract":[],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/AppliedStatus","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"AppliedStatus"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/appliedstatus"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes/ManagedResourceSummary":{"role":"symbol","title":"ARCZonalShiftClientTypes.ManagedResourceSummary","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"ManagedResourceSummary"}],"abstract":[{"type":"text","text":"A complex structure for a managed resource in an account. A managed resource is a Network Load Balancer or Application Load Balancer that has been registered with Route 53 ARC by Elastic Load Balancing. You can start a zonal shift in Route 53 ARC for a managed resource to temporarily move traffic for the resource away from an Availability Zone in an AWS Region. At this time, you can only start a zonal shift for Network Load Balancers and Application Load Balancers with cross-zone load balancing turned off."}],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/ManagedResourceSummary","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ManagedResourceSummary"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/managedresourcesummary"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes/ZonalShiftInResource":{"role":"symbol","title":"ARCZonalShiftClientTypes.ZonalShiftInResource","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"ZonalShiftInResource"}],"abstract":[{"type":"text","text":"A complex structure that lists the zonal shifts for a managed resource and their statuses for the resource."}],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/ZonalShiftInResource","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ZonalShiftInResource"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/zonalshiftinresource"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes/ZonalShiftStatus":{"role":"symbol","title":"ARCZonalShiftClientTypes.ZonalShiftStatus","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"ZonalShiftStatus"}],"abstract":[],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/ZonalShiftStatus","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ZonalShiftStatus"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/zonalshiftstatus"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift":{"role":"collection","title":"AWSARCZonalShift","abstract":[],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift","kind":"symbol","type":"topic","url":"\/documentation\/awsarczonalshift"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes/ZonalShiftSummary":{"role":"symbol","title":"ARCZonalShiftClientTypes.ZonalShiftSummary","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"ZonalShiftSummary"}],"abstract":[{"type":"text","text":"You start a zonal shift to temporarily move load balancer traffic away from an Availability Zone in a AWS Region. A zonal shift helps your application recover immediately, for example, from a developer’s bad code deployment or from an AWS infrastructure failure in a single Availability Zone. You can start a zonal shift in Route 53 ARC only for managed resources in your account in an AWS Region. Supported AWS resources are automatically registered with Route 53 ARC. Zonal shifts are temporary. A zonal shift can be active for up to three days (72 hours). When you start a zonal shift, you specify how long you want it to be active, which Amazon Route 53 Application Recovery Controller converts to an expiry time (expiration time). You can cancel a zonal shift, for example, if you’re ready to restore traffic to the Availability Zone. Or you can extend the zonal shift by updating the expiration so the zonal shift is active longer."}],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes\/ZonalShiftSummary","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ZonalShiftSummary"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes\/zonalshiftsummary"},"doc://AWSARCZonalShift/documentation/AWSARCZonalShift/ARCZonalShiftClientTypes":{"role":"symbol","title":"ARCZonalShiftClientTypes","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"ARCZonalShiftClientTypes"}],"abstract":[],"identifier":"doc:\/\/AWSARCZonalShift\/documentation\/AWSARCZonalShift\/ARCZonalShiftClientTypes","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ARCZonalShiftClientTypes"}],"url":"\/documentation\/awsarczonalshift\/arczonalshiftclienttypes"}}}