p","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"op"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"APIGatewayClientTypes","preciseIdentifier":"s:13AWSAPIGateway21APIGatewayClientTypesO"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Op","preciseIdentifier":"s:13AWSAPIGateway21APIGatewayClientTypesO2OpO"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"An update operation to be performed with this PATCH request. The valid value can be add, remove, replace or copy. Not all valid operations are supported for a given resource. Support of the operations depends on specific operational contexts. Attempts to apply an unsupported operation on a resource will return an error message.."}],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/PatchOperation\/op","kind":"symbol","type":"topic","url":"\/documentation\/awsapigateway\/apigatewayclienttypes\/patchoperation\/op"},"doc://AWSAPIGateway/Se":{"type":"unresolvable","title":"Swift.Decodable","identifier":"doc:\/\/AWSAPIGateway\/Se"},"https://docs.aws.amazon.com/apigateway/latest/api/patch-operations.html":{"title":"Patch Operations","titleInlineContent":[{"type":"text","text":"Patch Operations"}],"type":"link","identifier":"https:\/\/docs.aws.amazon.com\/apigateway\/latest\/api\/patch-operations.html","url":"https:\/\/docs.aws.amazon.com\/apigateway\/latest\/api\/patch-operations.html"},"doc://AWSAPIGateway/documentation/AWSAPIGateway/APIGatewayClientTypes":{"role":"symbol","title":"APIGatewayClientTypes","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"APIGatewayClientTypes"}],"abstract":[],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"APIGatewayClientTypes"}],"url":"\/documentation\/awsapigateway\/apigatewayclienttypes"},"doc://AWSAPIGateway/documentation/AWSAPIGateway/APIGatewayClientTypes/PatchOperation/Decodable-Implementations":{"role":"collectionGroup","title":"Decodable Implementations","abstract":[],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/PatchOperation\/Decodable-Implementations","kind":"article","type":"topic","url":"\/documentation\/awsapigateway\/apigatewayclienttypes\/patchoperation\/decodable-implementations"},"doc://AWSAPIGateway/documentation/AWSAPIGateway":{"role":"collection","title":"AWSAPIGateway","abstract":[],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway","kind":"symbol","type":"topic","url":"\/documentation\/awsapigateway"},"doc://AWSAPIGateway/documentation/AWSAPIGateway/APIGatewayClientTypes/PatchOperation/from":{"role":"symbol","title":"from","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"from"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"The copy update operation’s source as identified by a JSON-Pointer value referencing the location within the targeted resource to copy the value from. For example, to promote a canary deployment, you copy the canary deployment ID to the affiliated deployment ID by calling a PATCH request on a Stage resource with “op”:“copy”, “from”:”\/canarySettings\/deploymentId” and “path”:”\/deploymentId”."}],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/PatchOperation\/from","kind":"symbol","type":"topic","url":"\/documentation\/awsapigateway\/apigatewayclienttypes\/patchoperation\/from"},"doc://AWSAPIGateway/SE":{"type":"unresolvable","title":"Swift.Encodable","identifier":"doc:\/\/AWSAPIGateway\/SE"},"doc://AWSAPIGateway/documentation/AWSAPIGateway/APIGatewayClientTypes/PatchOperation/Equatable-Implementations":{"role":"collectionGroup","title":"Equatable Implementations","abstract":[],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/PatchOperation\/Equatable-Implementations","kind":"article","type":"topic","url":"\/documentation\/awsapigateway\/apigatewayclienttypes\/patchoperation\/equatable-implementations"},"doc://AWSAPIGateway/documentation/AWSAPIGateway/APIGatewayClientTypes/PatchOperation/Encodable-Implementations":{"role":"collectionGroup","title":"Encodable Implementations","abstract":[],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/PatchOperation\/Encodable-Implementations","kind":"article","type":"topic","url":"\/documentation\/awsapigateway\/apigatewayclienttypes\/patchoperation\/encodable-implementations"},"doc://AWSAPIGateway/documentation/AWSAPIGateway/APIGatewayClientTypes/PatchOperation/path":{"role":"symbol","title":"path","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"path"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"The op operation’s target, as identified by a JSON Pointer value that references a location within the targeted resource. For example, if the target resource has an updateable property of {“name”:“value”}, the path for this property is \/name. If the name property value is a JSON object (e.g., {“name”: {“child\/name”: “child-value”}}), the path for the child\/name property will be \/name\/child~1name. Any slash (”\/”) character appearing in path names must be escaped with “~1”, as shown in the example above. Each op operation can have only one path associated with it."}],"identifier":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/PatchOperation\/path","kind":"symbol","type":"topic","url":"\/documentation\/awsapigateway\/apigatewayclienttypes\/patchoperation\/path"}}}