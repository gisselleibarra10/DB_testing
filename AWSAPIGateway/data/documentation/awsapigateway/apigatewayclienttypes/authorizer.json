{"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Authorizer"}],"languages":["swift"],"platforms":["macOS"]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/awsapigateway\/apigatewayclienttypes\/authorizer"],"traits":[{"interfaceLanguage":"swift"}]}],"relationshipsSections":[{"identifiers":["doc:\/\/AWSAPIGateway\/Se","doc:\/\/AWSAPIGateway\/SE","doc:\/\/AWSAPIGateway\/SQ"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"identifier":{"url":"doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/Authorizer","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Represents an authorization layer for methods. If enabled on a method, API Gateway will activate the authorizer when a client calls the method."}],"kind":"symbol","metadata":{"navigatorTitle":[{"kind":"identifier","text":"Authorizer"}],"role":"symbol","title":"APIGatewayClientTypes.Authorizer","roleHeading":"Structure","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Authorizer"}],"symbolKind":"struct","externalID":"s:13AWSAPIGateway21APIGatewayClientTypesO10AuthorizerV","extendedModule":"AWSAPIGateway","modules":[{"name":"AWSAPIGateway"}]},"hierarchy":{"paths":[["doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway","doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes"]]},"topicSections":[{"title":"Initializers","identifiers":["doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/Authorizer\/init(authType:authorizerCredentials:authorizerResultTtlInSeconds:authorizerUri:id:identitySource:identityValidationExpression:name:providerARNs:type:)"]},{"title":"Instance Properties","identifiers":["doc:\/\/AWSAPIGateway\/documentation\/AWSAPIGateway\/APIGatewayClientTypes\/Authorizer\/authType","doc: