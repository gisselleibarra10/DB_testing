htsbyassessment\/assessmentcontrolscountbynoncompliantevidence"},"doc://AWSAuditManager/documentation/AWSAuditManager/AuditManagerClientTypes/InsightsByAssessment/totalAssessmentControlsCount":{"role":"symbol","title":"totalAssessmentControlsCount","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"totalAssessmentControlsCount"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"The total number of controls in the assessment."}],"identifier":"doc:\/\/AWSAuditManager\/documentation\/AWSAuditManager\/AuditManagerClientTypes\/InsightsByAssessment\/totalAssessmentControlsCount","kind":"symbol","type":"topic","url":"\/documentation\/awsauditmanager\/auditmanagerclienttypes\/insightsbyassessment\/totalassessmentcontrolscount"},"doc://AWSAuditManager/documentation/AWSAuditManager/AuditManagerClientTypes/InsightsByAssessment/noncompliantEvidenceCount":{"role":"symbol","title":"noncompliantEvidenceCount","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"noncompliantEvidenceCount"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"},{"kind":"text","text":"?"}],"abstract":[{"type":"text","text":"The number of compliance check evidence that Audit Manager classified as non-compliant. This includes evidence that was collected from Security Hub with a Fail ruling, or collected from Config with a Non-compliant ruling."}],"identifier":"doc:\/\/AWSAuditManager\/documentation\/AWSAuditManager\/AuditManagerClientTypes\/InsightsByAssessment\/noncompliantEvidenceCount","kind":"symbol","type":"topic","url":"\/documentation\/awsauditmanager\/auditmanagerclienttypes\/insightsbyassessment\/noncompliantevidencecount"},"doc://AWSAuditManager/documentation/AWSAuditManager/AuditManagerClientTypes/InsightsByAssessment/inconclusiveEvidenceCount":{"role"